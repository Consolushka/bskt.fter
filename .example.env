# DB
DB_HOST=db
DB_DRIVER=postgres
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=imp
DB_PORT=5432

# GOOSE
GOOSE_DRIVER=postgres
GOOSE_DBSTRING=postgres://postgres:postgres@db:5432/imp
GOOSE_MIGRATION_DIR=./app/database/migrations

# LOGGER (legacy values from current .env)
