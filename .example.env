LOGGER_FILE_PATH=tmp/logs/app.log
LOGGER_FILE_LEVEL=info
LOGGER_TELEGRAM_ENABLED=false
LOGGER_TELEGRAM_TOKEN=
LOGGER_TELEGRAM_CHAT_ID=
LOGGER_TELEGRAM_LEVEL=warn

# PROVIDERS
NBA_STATS_PROVIDER=CND.NBA

# API-SPORTS
API_SPORT_API_KEY=change_me
